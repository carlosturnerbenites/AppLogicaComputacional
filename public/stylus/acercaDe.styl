|	Pagina -> Acerca de	|
+-----------------------+
*/
.acercaDe
	.participante
		display flex
		border 1px solid rgba(0,0,0,.1)
		border-radius 10px
		flex-wrap  wrap
		width 300px
		margin 10px 30px
		.img
			width 100%
			img
				display block
				width 100%
				margin 0 auto
				-webkit-filter grayscale(60%)
				transition all 1s linear
				&:hover
					-webkit-filter grayscale(0%)

		.infoCreador
			padding 10px
			width 100%
			ul
				margin-left 30px
				list-style square
